{
  "name": "epsilon-dsva-react-exercise",
  "version": "1.0.0",
  "description": "A Skeleton Visualization application created by the Epsilon DSVA Team",
  "author": "Epsilon DSVA Team",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "concurrently -n server,react -c green,blue \"yarn dev:server\" \"yarn dev:app\"",
    "dev:app": "cross-env NODE_ENV=development webpack serve --open --config webpack.config.js",
    "dev:server": "cross-env NODE_ENV=development webpack --config webpack.config.server.js",
    "build": "yarn build:app && yarn build:server",
    "build:app": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "watch:server": "cross-env NODE_ENV=development nodemon dist/index.js",
    "serve": "node dist/index.js",
    "gen-data": "node data/genData.js"
  },
  "dependencies": {
    "d3": "7.8.5",
    "express": "4.18.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/d3": "7.4.1",
    "@types/express": "^4.17.19",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "chance": "^1.1.11",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "dotenv-webpack": "^8.0.1",
    "html-webpack-plugin": "^5.5.3",
    "nodemon": "^3.0.1",
    "sass": "^1.69.4",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.3.1"
  }
}
